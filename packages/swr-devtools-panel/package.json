{
  "name": "swr-devtools-panel",
  "version": "0.4.0",
  "description": "A React component for the SWR DevTools panel",
  "main": "cjs/index.js",
  "module": "lib/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "types": "lib/index.d.ts",
  "license": "MIT",
  "files": [
    "lib",
    "cjs"
  ],
  "author": "koba04",
  "scripts": {
    "clean": "rimraf lib cjs",
    "start": "run-p start:*",
    "start:cjs": "tsc -p tsconfig.cjs.json --watch",
    "start:esm": "tsc --watch",
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "styled-components": "^5.3.0",
    "swr": ">=1.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.16",
    "@types/styled-components": "^5.1.12",
    "react": "^17.0.2",
    "styled-components": "^5.3.0",
    "swr": "^2.0.0-beta.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "react-json-view": "^1.21.3",
    "swr-devtools": "^0.4.0"
  }
}
